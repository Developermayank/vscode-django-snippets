{
    "name": "vscode-django-snippets",
    "displayName": "VsCode - Django Snippets",
    "description": "A collection of snippets for django templates, models, views, fields & forms",
    "version": "1.0.0",
    "publisher": "developermayank",
    "icon": "images/dm_icon.png",
    "galleryBanner": {
        "color": "#0C4B33",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "snippets",
        "python",
        "django"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:Developermayank/vscode-django-snippets.git"
    },
    "contributes": {
        "languages": [
            {
                "id": "django-html",
                "aliases": [
                    "Django Template"
                ],
                "extensions": [".html", ".htm"],
                "configuration": "./django-html.configuration.json"
			},
			{
                "id": "html",
                "extensions": [".html", ".htm"]
            }


        ],
        "grammars": [
            {
                "language": "django-html",
                "scopeName": "text.html.django",
                "path": "./syntaxes/django-html.json"
            }
        ],
        "snippets": [
		{
			"language": "python",
			"path": "./snippets/admin/classes.json"
		},
		{
			"language": "python",
			"path": "./snippets/admin/options.json"
		},
		{
			"language": "python",
			"path": "./snippets/admin/imports.json"
		},
		{
			"language": "python",
			"path": "./snippets/admin/methods.json"
		},
		{
			"language": "python",
			"path": "./snippets/forms/classes.json"
		},
		{
			"language": "python",
			"path": "./snippets/forms/fields.json"
		},
		{
			"language": "python",
			"path": "./snippets/forms/imports.json"
    		},
		{
			"language": "python",
			"path": "./snippets/forms/methods.json"
		},
		{
			"language": "python",
			"path": "./snippets/models/classes.json"
		},
		{
			"language": "python",
			"path": "./snippets/models/fields.json"
		},
		{
			"language": "python",
			"path": "./snippets/models/imports.json"
		},
		{
			"language": "python",
			"path": "./snippets/models/methods.json"
		},
		{
			"language": "python",
			"path": "./snippets/python/classes.json"
		},
		{
			"language": "python",
			"path": "./snippets/python/python.json"
		},
		{
			"language": "python",
			"path": "./snippets/views/classes.json"
		},
		{
			"language": "python",
			"path": "./snippets/views/methods.json"
		},
		{
			"language": "python",
			"path": "./snippets/views/imports.json"
		},
		{
			"language": "python",
			"path": "./snippets/templates/methods.json"
		},
		{
			"language": "python",
			"path": "./snippets/templates/imports.json"
		},
		{
			"language": "html",
			"path": "./snippets/templates/html.json"
		},
		{
			"language": "django-html",
			"path": "./snippets/templates/html.json"
		},
		{
			"language": "python",
			"path": "./snippets/urls/imports.json"
		},
		{
			"language": "python",
			"path": "./snippets/urls/methods.json"
		},
		{
			"language": "python",
			"path": "./snippets/urls/regexes.json"
		}
        ]
    }
}
